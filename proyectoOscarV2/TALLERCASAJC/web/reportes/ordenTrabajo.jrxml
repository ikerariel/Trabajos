	</parameter>
	<queryString>
		<![CDATA[SELECT ot.id_ordenttabajo, ot.fecha::date,  ot.orden, ot.id_garantia, ot.id_usuario, 
                          ot.id_estado, d.diagnostico, (p.fecha) as fechapresupuesto, ot.id_presuserv,
 ot.fechaentrega::date, (c.ruc||' / '||c.razonsocial) as cliente, (pd.id_articulo) as codRepuesto, (pd.cantidad) as cantRespuesto, (a.art_descripcion) as respuesto, (r.observacion) as recepcion,
			(dr.id_articulo) as codaritucloRecepcion, (dr.cantidad) as cantidadarticuloRecepcion, (dra.art_descripcion) as articulo
                    FROM public.ordentrabajos ot
                    inner join presupuestosservicios p on ot.id_presuserv= p.id_presuserv
                    inner join detpresupuestosservicios pd on p.id_presuserv = pd.id_presuserv
                    inner join articulos a on pd.id_articulo = a.id_articulo
                    inner join diagnosticos d on p.id_diagnostico=d.id_diagnostico 
                    inner join  recepciones r on d.id_recepcion=r.id_recepcion
		    inner join detrecepciones dr on r.id_recepcion = dr.id_recepcion
		    inner join articulos dra on dr.id_articulo=dra.id_articulo
                    inner join clientes c on r.id_cliente=c.id_cliente
                    where ot.id_ordenttabajo =$P{id_ordenttabajo}]]>
	</queryString>
