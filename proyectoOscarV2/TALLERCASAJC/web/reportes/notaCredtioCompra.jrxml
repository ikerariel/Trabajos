	<parameter name="id_notacrecompra" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT nc.id_notacrecompra,nc.id_deposito, nc.fecha_nocred::date, nc.nro_nocred, nc.nro_timbrado, 
                                        nc.obs_nocred, nc.id_compra, (f.co_nrofact) as factura, 
                                       nc.id_usuario,(a.art_descripcion) as articulo, (u.usu_nombre) as usuarios, nc.id_estado, (e.est_descripcion) as estados,
                                       dnc. id_notacrecompra, dnc.id_articulo, dnc.cantidad_detnocre, dnc. montouni_detnocre, sum(dnc. montouni_detnocre*dnc.cantidad_detnocre) as subtotal   
                                       FROM notacrecompras nc
                                        INNER JOIN detnotacrecompras dnc on nc.id_notacrecompra = dnc.id_notacrecompra
                                        INNER JOIN articulos a on dnc.id_articulo=a.id_articulo
                                       INNER JOIN facturascompras f on nc.id_compra = f.id_compra
                                        INNER JOIN usuarios u on nc.id_usuario = u.id_usuario
                                      INNER JOIN estados e on nc.id_estado = e.id_estado
                                      where nc.id_notacrecompra = $P{id_notacrecompra}
group by nc.id_notacrecompra,nc.id_deposito, nc.fecha_nocred::date, nc.nro_nocred, nc.nro_timbrado, 
                                        nc.obs_nocred, nc.id_compra, f.co_nrofact, 
                                       nc.id_usuario,a.art_descripcion, nc.id_estado,e.est_descripcion,
                                       dnc. id_notacrecompra, dnc.id_articulo, dnc.cantidad_detnocre, dnc. montouni_detnocre,u.usu_nombre]]>
	</queryString>
