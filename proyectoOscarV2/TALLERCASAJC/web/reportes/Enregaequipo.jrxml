	<parameter name="id_entregaequipos" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT eq.id_entregaequipos, eq.fecha_entrega, eq.id_cliente, eq.id_usuario, eq.observacion, eq.id_ordenttabajo,(c.ruc||' / '||c.razonsocial) as cliente,
ot.orden,(pd.id_articulo) as codRepuesto, (pd.cantidad) as cantRespuesto,(pd.precio) as precioRespu, (a.art_descripcion) as respuesto, (pd.cantidad*pd.precio) as subtotalPresu,
	(dq.id_articulo) as codarticuloentrega, (dq.cantidad) as cantidadentrega, (dra.art_descripcion) as articulo, (cp.descripcion) as tipoPago
                    FROM public.entrega_equpos eq
                    left join detentregaequipos dq on eq.id_entregaequipos= dq.id_entregaequipos
  left join clientes c on eq.id_cliente=c.id_cliente
   left join ordentrabajos ot on ot.id_ordenttabajo= eq.id_ordenttabajo
 left join presupuestosservicios p on ot.id_presuserv= p.id_presuserv
 left join detpresupuestosservicios pd on p.id_presuserv = pd.id_presuserv
left join condicion_pago cp on p.id_condicionpago = cp.id_condicionpago
  left join articulos a on pd.id_articulo = a.id_articulo
 left join articulos dra on dq.id_articulo = dra.id_articulo
  where eq.id_entregaequipos =$P{id_entregaequipos}]]>
	</queryString>
