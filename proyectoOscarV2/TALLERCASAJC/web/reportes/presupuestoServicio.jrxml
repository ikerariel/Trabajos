	<queryString>
		<![CDATA[SELECT p.id_presuserv, p.id_estado, p.id_diagnostico, p.id_usuario, p.fecha::date,
p.observacion, (d.fecha::date) as fechadiagnostico, pd.id_articulo, pd.precio, pd.cantidad,
 (a.art_descripcion)as articulo,(pd.precio * pd.cantidad) as subtotal,
(c.ruc||' / '||c.razonsocial) as cliente, (cp.descripcion) as pago
  FROM public.presupuestosservicios p
  left join condicion_pago cp on p.id_condicionpago = cp.id_condicionpago
  left join detpresupuestosservicios pd on p.id_presuserv=pd.id_presuserv
  left join articulos a on pd.id_articulo=a.id_articulo
left join diagnosticos d on p.id_diagnostico = d.id_diagnostico
   left join recepciones r on d.id_recepcion=r.id_recepcion
left join clientes c on r.id_cliente=c.id_cliente
  where p.id_presuserv=$P{id_presuserv}]]>
	</queryString>
