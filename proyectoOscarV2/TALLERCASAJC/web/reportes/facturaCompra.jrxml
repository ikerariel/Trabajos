	<queryString>
		<![CDATA[SELECT f.co_fecha::date, f.id_compra,  o.id_ordcompra, f.co_nrofact, f.id_proveedor,  (p.ras_social) as proveedor, 
                    f.id_condicionpago,(cp.descripcion) as condpago, f.co_intervalo, f.co_cantcuota,  f.co_monto,    d.id_articulo,a.art_descripcion,
                    d.precio_detcomp, d.cantidad_detcomp,d.id_impuesto, sum(d.precio_detcomp*d.cantidad_detcomp) as subtotal
                     FROM facturascompras f
                     left join proveedores p on f.id_proveedor = p.id_proveedor
                     left join condicion_pago cp on f.id_condicionpago=cp.id_condicionpago	
                     left join ordencompras o on f.id_ordcompra = o.id_ordcompra
                     left join detfacturascompras d on f.id_compra = d.id_compra
                    left join articulos a on d.id_articulo = a.id_articulo
                     where f.id_compra=$P{id_compra}
group by  f.co_fecha::date, f.id_compra,  o.id_ordcompra, f.co_nrofact, f.id_proveedor,p.ras_social,
                    f.id_condicionpago,cp.descripcion,f.co_intervalo, f.co_cantcuota,  f.co_monto,    d.id_articulo,a.art_descripcion,
                    d.precio_detcomp, d.cantidad_detcomp,d.id_impuesto]]>
	</queryString>
