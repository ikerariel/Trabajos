	<parameter name="id_notadecompra" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT n.id_notadecompra, n.fecha::date, n.nro_notadebito, n.nro_timbradonotadebito, 
                    n.id_compra, (f.co_nrofact) as factura, n.id_estado, (e.est_descripcion) as estado,
                     n.id_usuario, (u.usu_nombre) as usuario, d.importe, UPPER(d.conceptos) as concepto, n.observacion
                    FROM notadecompras n
                    INNER JOIN detnotadecompras d on n.id_notadecompra = d.id_notadecompra
                    INNER JOIN facturascompras f on n.id_compra = f.id_compra
                    INNER JOIN usuarios u on n.id_usuario = u.id_usuario
                    INNER JOIN estados e on n.id_estado = e.id_estado
                    where n.id_notadecompra = $P{id_notadecompra}]]>
	</queryString>
