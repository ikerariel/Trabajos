	<parameter name="id_ajuste" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT a.id_ajuste, a.fecha_ajustes::date, a.id_sucursal, a.id_deposito, a.id_usuario, 
                          a.id_estado, upper(a.observacion) as observacion, a.id_tipajuste, t.descripcion, d.id_articulo, ar.art_descripcion, d.cantexistencia,
                          (dp.dep_descripcion) as deposito
                    FROM ajustes a
                   LEFT JOIN detajustes d on a.id_ajuste = d.id_ajuste
                    LEFT join articulos ar on d.id_articulo = ar.id_articulo
                    LEFT Join tipoajustes t on a.id_tipajuste = t.id_tipajuste
                    left join depositos dp on a.id_deposito=dp.id_deposito
                    where a.id_ajuste =$P{id_ajuste}]]>
	</queryString>
