	<parameter name="id_estado" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="vUser" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fdesde" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fhasta" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT $P{vUser} as usuario,f.id_compra, f.co_nrofact, f.co_fecha, p.ras_social,$P{fdesde}::date as fechadessde, $P{fhasta}::date as fechahasta,(e.est_descripcion) as estado,
coalesce(a.total,0) as totalporfactura
	FROM public.facturascompras f
	left join proveedores p on  f.id_proveedor = p.id_proveedor
          left join estados e on f.id_estado=e.id_estado
	left join (select f.id_compra, sum(d.cantidad_detcomp*d.precio_detcomp)as total
	from facturascompras f
	left join detfacturascompras d on f.id_compra=d.id_compra
          where  f.id_estado=1 and f.co_fecha::date between $P{fdesde}::date and  $P{fhasta}::date
	group by f.id_compra) a  on a.id_compra = f.id_compra
        where f.id_estado=1 and f.co_fecha::date between $P{fdesde}::date and  $P{fhasta}::date
	group by  f.co_nrofact, coalesce(a.total, 0),e.est_descripcion,
	f.id_compra, f.co_fecha, p.ras_social
	order by f.co_nrofact asc]]>
	</queryString>
