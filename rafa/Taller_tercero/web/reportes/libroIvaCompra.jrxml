	<parameter name="vUser" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT $P{vUser} as usuario,f.idcompra, f.comp_nrofact, f.comp_fecha, p.prov_nombre,
coalesce(a.total,0) as totalporfactura,coalesce(i.total10,0) as total10,coalesce(im.total5,0) as total5, coalesce(imp.totalexe,0) as totalexe,
coalesce(grb.grav10,0) as grav10, coalesce(grba.grav5,0) as grav5
	FROM public.factura_compra f
	left join proveedores p on  f.id_prov = p.id_prov
	left join (select f.idcompra, sum(d.detfact_cantidad*d.detfact_precio)as total 
            from factura_compra f
	    left join det_factura_compra d on f.idcompra=d.idcompra
             where f.idestado=2 group by f.idcompra) a  on a.idcompra = f.idcompra
left join (select f.idcompra, sum(d.detfact_cantidad*d.detfact_precio)as total10 
            from factura_compra f
	    left join det_factura_compra d on f.idcompra=d.idcompra
	             where f.idestado=2 and d.idimpuesto= 1 group by f.idcompra) i  on i.idcompra = f.idcompra
left join (select f.idcompra, sum(d.detfact_cantidad*d.detfact_precio)as total5 
            from factura_compra f
	    left join det_factura_compra d on f.idcompra=d.idcompra
	             where f.idestado=2 and d.idimpuesto= 2 group by f.idcompra) im  on im.idcompra = f.idcompra
left join (select f.idcompra, sum(d.detfact_cantidad*d.detfact_precio)as totalexe 
            from factura_compra f
	    left join det_factura_compra d on f.idcompra=d.idcompra
	             where f.idestado=2 and d.idimpuesto= 3 group by f.idcompra) imp  on imp.idcompra = f.idcompra
left join (select f.idcompra, sum(d.detfact_cantidad*d.detfact_precio)/11as grav10 
            from factura_compra f
	    left join det_factura_compra d on f.idcompra=d.idcompra
	             where f.idestado=2 and d.idimpuesto= 1 group by f.idcompra) grb  on grb.idcompra = f.idcompra
left join (select f.idcompra, sum(d.detfact_cantidad*d.detfact_precio)/21as grav5 
            from factura_compra f
	    left join det_factura_compra d on f.idcompra=d.idcompra
	             where f.idestado=2 and d.idimpuesto= 2 group by f.idcompra) grba  on grba.idcompra = f.idcompra
	where f.idestado=2
	group by  f.comp_nrofact, coalesce(a.total, 0),
	f.idcompra, f.comp_fecha, p.prov_nombre,
	coalesce(i.total10,0),total10,coalesce(im.total5,0),coalesce(imp.totalexe,0),coalesce(grb.grav10,0),coalesce(grba.grav5,0) 
	order by f.comp_nrofact asc]]>
	</queryString>
