	<queryString>
		<![CDATA[SELECT o.ordenc_nro, o.ordenc_fecha, o.id_prov, (p.prov_nombre) as proveedor, o.idusuario, o.pcomp_nro, o.idestado,d.idmercaderia,
d.cant_orden, d.precio_orden, sum(d.cant_orden*d.precio_orden) as subtotal, (m.mer_descripcion)as mercaderia
  FROM public.orden_compra o
left join det_orden_compra d on o.ordenc_nro=d.ordenc_nro
left join mercaderias m on d.idmercaderia=m.idmercaderia
LEFT JOIN proveedores p on o.id_prov=p.id_prov
where o.ordenc_nro=$P{ordenc_nro}
group by o.ordenc_nro, o.ordenc_fecha, o.id_prov, o.idusuario, o.pcomp_nro, o.idestado,
d.cant_orden, d.precio_orden, m.mer_descripcion, p.prov_nombre,d.idmercaderia]]>
	</queryString>
