	<queryString>
		<![CDATA[SELECT a.idajuste, a.ajuste_fecha, a.idmot_ajus, a.idusuario, a.idestado, a.idtipo_ajuste, 
       a.iddeposito, (t.descripcion) as ajuste, (d.dep_descripcion) as deposito, (mt.ajustmotiv_descri) as motivo,
       da.idmercaderia, da.ajuste_cantidad, (m.mer_descripcion) as mercaderia
  FROM public.ajuste a
left join detalle_ajuste da on a.idajuste=da.idajuste
left join mercaderias m on da.idmercaderia=m.idmercaderia
left join tipo_ajuste t on a.idtipo_ajuste=t.idtipo_ajuste
left join depositos d on a.iddeposito=d.iddeposito
left join motivo_ajuste mt on  a.idmot_ajus=mt.idmot_ajus
where a.idajuste=$P{idajuste}]]>
	</queryString>
