	<parameter name="idapertura" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT concat(cl.cedula||' / '|| cl.nombrecliente) as cliente, e.idapertura, (e.fecha_apertura) as fechaApertura,e.fechacierre, r.fecha, (o.descripcion) as operadora, 
(n.descripcion) as operacion,(us.usuario) as usuario,
concat('0',r.nroorigen) as nroorigen, concat('0',r.nrodestino) as nrodestino,
r.montogirobilletera, r.porcentaje, r.montototaenvio,
(select sum(d.montototaenvio) as totaldif from detallegiro_billetera d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
left join tipooperacion t on d.idtipooperacion = t.idtipooperacion
where e.idapertura=$P{idapertura} and d.idestado in(6,8) and d.idestado in(6,8)),
(select sum(d.montototaenvio) as total from detallegiro_billetera d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where e.idapertura=$P{idapertura}),
coalesce(a.claro,0) as claro,
coalesce(b.personal,0) as personal,
coalesce(c.vox,0) as vox,
coalesce(d.tigo, 0) as tigo, 
coalesce(f.giro, 0) as giro,
coalesce(h.billetera, 0) as billetera,
coalesce(k.efectivizado, 0) as efectivizado, 
coalesce(z.revertido, 0) as revertido,
coalesce(v.retirobilletera,0) as retirobilletera, 
coalesce(s.operpagadas,0) as operpagadas,
coalesce(t.operpendientes,0)as operpendientes, (select count(*)from detallegiro_billetera where idapertura=$P{idapertura}) 
as cantregistros, (p.descripcion) as pago
FROM apertura_recarga_giro e 
left join detallegiro_billetera r on e.idapertura = r.idapertura
left join tipooperadora o on r.idtipooperadora=o.idtipooperadora
left join tipooperacion n on r.idtipooperacion = n.idtipooperacion
left join usuario us on e.idusuario=us.idusuario
left join (select idapertura, sum(montototaenvio) as claro 
	      from detallegiro_billetera 
	     where idtipooperadora = 1 
	  group by idapertura) a on a.idapertura = e.idapertura
left join (select idapertura, sum(montototaenvio) as personal 
	      from detallegiro_billetera 
	     where idtipooperadora = 2 
	  group by idapertura) b on b.idapertura = e.idapertura
left join (select idapertura, sum(montototaenvio) as vox
	      from detallegiro_billetera 
	     where idtipooperadora = 3 
	  group by idapertura) c on c.idapertura = e.idapertura
left join (select idapertura, sum(montototaenvio) as tigo
	      from detallegiro_billetera 
	     where idtipooperadora = 4 
	  group by idapertura) d on d.idapertura = e.idapertura	 
left join (select idapertura, sum(montototaenvio) as giro
	      from detallegiro_billetera 
	     where idtipooperacion = 1 
	  group by idapertura) f on f.idapertura = e.idapertura	
left join (select idapertura, sum(montototaenvio) as billetera
	      from detallegiro_billetera 
	     where idtipooperacion = 2 
	  group by idapertura) h on h.idapertura = e.idapertura	
left join (select idapertura, sum(montototaenvio) as efectivizado
	      from detallegiro_billetera 
	     where idtipooperacion = 3 
	  group by idapertura) k on k.idapertura = e.idapertura	  
	  left join (select idapertura, sum(montototaenvio) as revertido
	      from detallegiro_billetera 
	     where idtipooperacion = 4 
	  group by idapertura) z on z.idapertura = e.idapertura

  left join (select idapertura, sum(montototaenvio) as retirobilletera
	      from detallegiro_billetera 
	     where idtipooperacion = 5 
	  group by idapertura) v on v.idapertura = e.idapertura

  left join (select idapertura, sum(montototaenvio) as operpagadas
	      from detallegiro_billetera 
	     where idestado = 6 
	  group by idapertura) s on s.idapertura = e.idapertura

 left join (select idapertura, sum(montototaenvio) as operpendientes
	      from detallegiro_billetera 
	     where idestado = 7 
	  group by idapertura) t on s.idapertura = e.idapertura	
left join estado p on r.idestado=p.idestado
left join cliente cl on r.idcliente=cl.idcliente       	  		  
where  e.idapertura=$P{idapertura}
group by e.idapertura,e.fecha_apertura,r.nrodestino, r.montototaenvio,o.descripcion,r.fecha,
 coalesce(a.claro,0), coalesce(b.personal,0), 
coalesce(c.vox,0), coalesce(d.tigo, 0),coalesce(f.giro, 0), coalesce(h.billetera, 0),coalesce(k.efectivizado, 0),coalesce(z.revertido, 0),coalesce(v.retirobilletera,0),coalesce(s.operpagadas,0),coalesce(t.operpendientes,0), n.idtipooperacion, r.nroorigen, r.montogirobilletera, r.porcentaje, p.descripcion,cl.cedula,cl.nombrecliente, us.usuario 
order by r.fecha desc]]>
	</queryString>
