	<queryString>
		<![CDATA[SELECT g.iddetallegiro, concat('0',g.nrodestino) as nrodestino ,concat('0',g.nroorigen) as nroorigen,  (o.descripcion) as operadora,
                    (p.descripcion) as operacion, g.montogirobilletera, 
                         g.nrotransaccion, g.nrooperacion, g.idcliente, (c.nombrecliente) as cliente, (c.cedula)as cedula, g.porcentaje, 
                         g.montototaenvio, g.fecha
                      FROM detallegiro_billetera g
                      LEFT JOIN tipooperadora o on g.idtipooperadora = o.idtipooperadora
                      LEFT JOIN tipooperacion p on g.idtipooperacion = p.idtipooperacion
		      LEFT JOIN cliente c on g.idcliente=c.idcliente	
	where g.iddetallegiro =$P{iddetallegiro}]]>
	</queryString>
