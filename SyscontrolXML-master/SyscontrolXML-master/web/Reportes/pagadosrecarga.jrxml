	<queryString>
		<![CDATA[SELECT pg.fechapagorecarga,pg.idestado,r.iddetallerecargas,concat(cl.cedula||' / '|| cl.nombrecliente) as cliente, e.idapertura,$P{vUser} as usuario,
                                      (e.fecha_apertura) as fechaApertura,e.fechacierre, r.fecharecarga, (o.descripcion) as operadora,'Recarga de Saldo' as operacion, 
                                      (p.descripcion)as estado,
                                       concat('0',r.nrodestino) as nrodestino,
                                       pg.montorecarga, pg.montopagadorecarga,
                                       $P{fdesde}::Date as fechadesde, $P{fhasta}::Date as fechahasta
                                       FROM apertura_recarga_giro e 
                                       left join detallerecargas r on e.idapertura = r.idapertura
                                        left join tipooperadora o on r.idtipooperadora=o.idtipooperadora
                                       left join estado p on r.idestado=p.idestado
                                       left join cliente cl on r.idcliente=cl.idcliente 
                                      left join pagorecarga pg on r.iddetallerecargas=pg.iddetallerecargas 	
				where  pg.idestado=6 and e.idestado in(1,2)  and  pg.fechapagorecarga::date
 BETWEEN $P{fdesde}::Date and $P{fhasta}::Date  order by r.fecharecarga desc]]>
	</queryString>
