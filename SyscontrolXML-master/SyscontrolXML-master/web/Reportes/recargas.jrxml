	<parameter name="idapertura" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT concat(cl.cedula||' / '|| cl.nombrecliente) as cliente,e.idapertura, (e.fecha_apertura) as fechaApertura, e.fechacierre,r.fecharecarga, (o.descripcion) as operadora, concat('0',r.nrodestino) as nrodestino,
r.montorecarga,(us.usuario) as usuario,
(select sum(d.montorecarga) as total from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where e.idapertura=$P{idapertura}),
coalesce(a.claro,0) as claro,
coalesce(b.personal,0) as personal,
coalesce(c.vox,0) as vox,
coalesce(d.tigo, 0) as tigo,
coalesce(g.rpagados,0)as rpagados,
coalesce(k.pagospend,0) as pagospend, r.observacion,(select count(*)from detallerecargas where idapertura=$P{idapertura} ) as cantregistros, (p.descripcion) as pago
FROM apertura_recarga_giro e 
left join detallerecargas r on e.idapertura = r.idapertura
left join tipooperadora o on r.idtipooperadora=o.idtipooperadora
left join usuario us on e.idusuario = us.idusuario
left join (select idapertura, sum(montorecarga) as claro 
	      from detallerecargas 
	     where idtipooperadora = 1 
	  group by idapertura) a on a.idapertura = e.idapertura
left join (select idapertura, sum(montorecarga) as personal 
	      from detallerecargas 
	     where idtipooperadora = 2 
	  group by idapertura) b on b.idapertura = e.idapertura
left join (select idapertura, sum(montorecarga) as vox
	      from detallerecargas 
	     where idtipooperadora = 3 
	  group by idapertura) c on c.idapertura = e.idapertura
left join (select idapertura, sum(montorecarga) as tigo
	      from detallerecargas 
	     where idtipooperadora = 4 
	  group by idapertura) d on d.idapertura = e.idapertura	
left join (select idapertura, sum(montorecarga) as rpagados
	      from detallerecargas 
	     where idestado = 6 
	  group by idapertura) g on g.idapertura = e.idapertura
left join (select idapertura, sum(montorecarga) as pagospend
	      from detallerecargas 
	     where idestado = 7 
	  group by idapertura) k on k.idapertura = e.idapertura		  	
left join cliente cl on r.idcliente=cl.idcliente  
left join estado p on r.idestado=p.idestado 		  
where  e.idapertura=$P{idapertura}
group by e.idapertura,e.fecha_apertura,r.nrodestino, r.montorecarga,o.descripcion,r.fecharecarga,
 coalesce(a.claro,0), coalesce(b.personal,0), us.usuario,
coalesce(c.vox,0), coalesce(d.tigo, 0),coalesce(g.rpagados,0),coalesce(k.pagospend,0),cl.cedula,cl.nombrecliente ,r.observacion, p.descripcion order by r.fecharecarga desc]]>
	</queryString>
