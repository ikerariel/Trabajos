	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fdesde" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fhasta" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="vUser" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT concat(cl.cedula||' / '|| cl.nombrecliente) as cliente,e.idapertura, (e.fecha_apertura) as fechaApertura, $P{vUser} as usuario,
e.fechacierre,r.fecharecarga, (o.descripcion) as operadora, concat('0',r.nrodestino) as nrodestino,
r.montorecarga,$P{fdesde}::date as fechadesde,  $P{fhasta}::date as fechahasta,
(select sum(d.montorecarga) as total from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2 ),
(select coalesce(sum(d.montorecarga),0) as claro 
from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.idtipooperadora = 1 and d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2),
(select coalesce(sum(d.montorecarga),0) as personal 
from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.idtipooperadora = 2 and d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2 ),
(select coalesce(sum(d.montorecarga),0) as vox
from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.idtipooperadora = 3 and d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2),
(select coalesce(sum(d.montorecarga),0) as tigo
from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.idtipooperadora = 4 and d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2),
(select coalesce(sum(d.montorecarga),0) as rpagados
from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.idestado = 6 and d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2 ),
(select coalesce(sum(d.montorecarga),0) as pagospend
from detallerecargas d
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where d.idestado = 7 and d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2),
r.observacion,
(select count(*)from detallerecargas d 
left join apertura_recarga_giro e on  d.idapertura = e.idapertura
where  d.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2) as cantregistros,
(p.descripcion) as pago
FROM apertura_recarga_giro e 
left join detallerecargas r on e.idapertura = r.idapertura
left join tipooperadora o on r.idtipooperadora=o.idtipooperadora
left join cliente cl on r.idcliente=cl.idcliente  
left join estado p on r.idestado=p.idestado 		  
where r.fecharecarga::date between $P{fdesde}::date and  $P{fhasta}::date and e.idestado = 2
group by e.idapertura,e.fecha_apertura,r.nrodestino, r.montorecarga,o.descripcion,r.fecharecarga,cl.cedula,cl.nombrecliente ,
r.observacion, p.descripcion order by r.fecharecarga desc]]>
	</queryString>
