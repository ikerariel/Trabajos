	<queryString>
		<![CDATA[select p.idpagorecarga, p.montorecarga, p.montopagadorecarga, p.fechapagorecarga, p.nrooperarecarga, 'Recarga de Saldo' as operacion,  
(c.nombrecliente) as cliente, (c.cedula)as cedula
from pagorecarga p
left join detallerecargas b on p.iddetallerecargas= b.iddetallerecargas
left join cliente c on b.idcliente=c.idcliente 
where p.idpagorecarga = $P{idpagorecarga}]]>
