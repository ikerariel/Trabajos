	<queryString>
		<![CDATA[SELECT t.idtimbrado, t.numero, t.fecha_carga, t.fecha_vigencia_inicio, t.fecha_vigencia_final, 
       t.factura_establesimiento, t.factura_caja, t.factura_desde, t.factura_hasta, $P{vUser} as usuario,
       t.idestado, t.idusuario, t.idtipodocumento, t.idsucursal, (e.descripcion) as estadoTimbrado,
       f.secuencia, f.numerodocumento, (ed.descripcion) as estadoDocumento, (tp.descripcion) as tipodocumento
  FROM ventas.timbrado t
  left join estado e on t.idestado=e.idestado
left join ventas.documentos_facturas f on t.idtimbrado=f.idtimbrado
  left join estado ed on f.idestado=ed.idestado
left join ventas.tipo_documento tp on t.idtipodocumento = tp.idtipodocumento
 where t.idtimbrado = $P{idtimbrado}order by f.secuencia asc]]>
	</queryString>
